[MASTER]
jobs = 0
persistent = no

[MESSAGES CONTROL]
enable=
	deprecated-pragma,
	useless-suppression,
	use-symbolic-message-instead,

disable=
	line-too-long,
	no-else-raise,
	no-else-return,
	too-few-public-methods,
	too-many-branches,
	too-many-instance-attributes,
	too-many-locals,
	too-many-nested-blocks,
	too-many-statements,
